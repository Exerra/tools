---
import Layout from "../../../layouts/Layout.astro"
import Header from "../../../components/Header.astro"

import ARC, { title, description } from "./_ARC"

---

<Layout
		title={`${title} | Exerra Tools`}
		description={description}
>
	<section class="container grid items-center gap-6 pt-6 pb-8 mx-auto px-6 md:py-10">
		<Header
				title={title}
				description={description}
		/>

		<ARC client:load />
	</section>

</Layout>